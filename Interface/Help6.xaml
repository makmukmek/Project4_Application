<Window x:Class="Interface.Help6"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="Задача на тему делителей. Справка." Height="409" Width="800" Icon="/helpbutton_85772.png">
    <Grid >
        <Label Content="Задача на тему делителей" Height="41" VerticalAlignment="Top" FontSize="24" HorizontalAlignment="Center" FontStyle="Italic"/>

        <Button Content="Закрыть" Click="CloseButton_Click" HorizontalAlignment="Center" Margin="0,0,0,10" Width="204" FontSize="24" FontStyle="Italic" Height="62" VerticalAlignment="Bottom"/>
        <TextBlock TextWrapping="Wrap" FontSize="24" HorizontalAlignment="Center" Margin="0,44,0,0" VerticalAlignment="Top" Height="272" Width="688" FontStyle="Italic"><Run Text="В заданном интервале [start, end] найти все числа, у которых количество делителей равно заданному числу (condition).Вывести по одному максимальному делителю для каждого из найденных чисел, не считая его самого. Переменная start содержит начало интервала, а переменная end - конец интервала, condition - число-условие, которому должно равняться количество делителей числа из интервала."/></TextBlock>

    </Grid>
</Window>
